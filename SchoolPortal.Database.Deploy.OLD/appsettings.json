{
  "ExecutionTimeoutSeconds": 600,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting": "Information",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Datadog.Logs" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "DatadogLogs",
        "Args": {
          "apiKey": "xxxxxxxxxxxxxxxxxxxxxxxxxxx",
          "service": "EducationInstitutions.Data",
          "use_http": true,
          "tags": [ "env:build" ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithExceptionDetails" ],
    "Properties": {
      "Service": "educationinstitutionsdata",
      "Environment": "build"
    }
  },
  "ConnectionStrings": {
    "DatabaseConnection": "Server=.;Database=SchoolPortal-SynonymId-to-Name;User=sa;Password=yourStrong(!)Password;Integrated Security=false;Encrypt=false;TrustServerCertificate=false;"
    //, "MasterConnection": "Server=.;Database=master;User=sa;Password=yourStrong(!)Password;Integrated Security=false;Encrypt=false;TrustServerCertificate=false;"
  }
}
